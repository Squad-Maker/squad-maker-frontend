{
  "name": "squad-maker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "preview": "vite preview",
    "proto:clear": "rm -Rf src/grpc/generated && mkdir -p src/grpc/generated",
    "proto:generate": "protoc --plugin=node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=src/grpc/generated --ts_proto_opt=outputClientImpl=grpc-web,forceLong=string,lowerCaseServiceMethods=true,esModuleInterop=true -Isrc/grpc/proto/api -Isrc/grpc/proto/third_party src/grpc/proto/api/**/*.proto",
    "proto:generate-netlify": "~/protoc/bin/protoc --plugin=node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=src/grpc/generated --ts_proto_opt=outputClientImpl=grpc-web,forceLong=string,lowerCaseServiceMethods=true,esModuleInterop=true -Isrc/grpc/proto/api -Isrc/grpc/proto/third_party src/grpc/proto/api/**/*.proto"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@tanstack/react-query": "^5.59.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.0",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^6.27.0",
    "rxjs": "^7.8.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/node": "^22.7.6",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-input-mask": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "ts-proto": "^1.181.2",
    "typescript": "5.3",
    "vite": "^5.4.8"
  }
}
